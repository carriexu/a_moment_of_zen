<a href="/">Home</a>
<section class="nytimes">
  <h2>NYTimes Most Popular Stories</h2>

  <% @parsed_response["results"].each do |article| %>
    <a href="<%= article["url"] %>"><%= article["title"] %></a>
    <p><%= article["abstract"] %></p>
    <br></br>
  <% end %>
</section>

<section class="weather">
  <p>The temperature is <%= @temp_in_f %> degrees.</p>
</section>

